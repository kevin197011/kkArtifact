# 存储配置（本地文件系统）
STORAGE_TYPE=local
STORAGE_BASE_PATH=~/kkartifact-storage
STORAGE_LOCAL_PATH=~/kkartifact-storage

# 日志配置
LOG_LEVEL=info
LOG_FORMAT=text

# 版本保留
VERSION_RETENTION_LIMIT=5
