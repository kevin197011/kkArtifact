# 本地开发环境变量
SERVER_HOST=0.0.0.0
SERVER_PORT=8080

# 数据库配置（Docker Compose 服务）
DB_HOST=localhost
DB_PORT=5432
DB_NAME=kkartifact
DB_USER=kkartifact
DB_PASSWORD=kkartifact
DB_SSLMODE=disable

# Redis 配置（Docker Compose 服务）
REDIS_HOST=localhost
